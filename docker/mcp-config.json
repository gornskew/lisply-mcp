{
    "mcpServers": {
        "gendl": {
            "command": "/home/dcooper8/projects/lisply-mcp/docker/mcp-exec",
            "args": [
                "--backend-host", "gendl-backend",
                "--mount", "/projects:/projects",
                "--mount", "/var/run/docker.sock:/var/run/docker.sock",
                "--mount", "/tmp/.X11-unix:/tmp/.X11-unix"
            ]
        },
        "emacs-cont": {
            "command": "/home/dcooper8/projects/lisply-mcp/docker/mcp-exec",
            "args": [
                "--server-name", "emacs-cont",
                "--http-host-port", "7081",
                "--http-port", "7080",
                "--image-base-name", "gornskew/skewed-emacs",
                "--backend-host", "emacs-backend",
                "--mount", "/projects:/projects",
                "--mount", "/tmp/.X11-unix:/tmp/.X11-unix",
                "--mount", "/var/run/docker.sock:/var/run/docker.sock"
            ]
        }
    }
}
